<% layout("./layouts/layout.eta") %>

<h1>Topics</h1>

<h2>Add topic</h2>

<form method="POST" action="/topics">
Topic:<br/>
<input type="text" name="name" /><br/>
<input type="submit" value="Add"/>
</form>

<ul>
<% it.topics.forEach(topic => { %>
    <li>
    <%= topic.name %>
    </li>
<%});%>
</ul>